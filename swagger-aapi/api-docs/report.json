{
    "apiVersion": "1",
    "swaggerVersion": "1.2",
    "basePath": "https://data.brightcove.com/analytics-api/videocloud",
    "resourcePath": "/account/{accountId}/report",
    "apis": [
        {
            "path": "/account/{accountId}/report",
            "description": "Generates reports on video analytics. ",
            "operations": [
                {
                    "httpMethod": "GET",
                    "summary": "Get an analytics report for the account",
                    "notes": "Returns an analytics report based on the dimensions and parameters specified",
                    "type": "report",
                    "nickname": "report",
                    "parameters": [
                        {
                            "name": "accountId",
                            "description": "ID of account",
                            "paramType": "path",
                            "required": true,
                            "allowMultiple": false,
                            "type": "string"
                        },
                        {
                            "name": "dimensions",
                            "type": "string",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "description": "The dimensions to report on. Specify one or more in a comma-separated list. Available dimensions are: <code>video</code>, <code>player</code>, <code>referrer_domain</code>, <code>search_terms</code>, <code>source_type</code>, <code>device_type</code>, <code>device_os</code>",
                            "allowableValues": {
                                "values": [
                                    "video",
                                    "player",
                                    "referrer_domain",
                                    "search_terms",
                                    "source_type",
                                    "device_type",
                                    "device_os"
                                ],
                                "valueType": "LIST"
                            }
                        },
                        {
                            "allowableValues": {
                                "values": [
                                    "active_media",
                                    "bytes_in",
                                    "bytes_out",
                                    "bytes_overhead",
                                    "bytes_stored",
                                    "bytes_player",
                                    "player_load",
                                    "video_impression",
                                    "video_view",
                                    "video_engagement",
                                    "video_percent_viewed"
                                ],
                                "valueType": "LIST"
                            },
                            "allowMultiple": false,
                            "type": "string",
                            "required": false,
                            "paramType": "query",
                            "description": "The metric to sort the results by",
                            "name": "sort"
                        },
                        {
                            "allowMultiple": false,
                            "type": "integer",
                            "required": false,
                            "minimum": 0,
                            "paramType": "query",
                            "description": "The number of results to skip &mdash; use with <code>device_os</code>",
                            "name": "offset"
                        },
                        {
                            "allowMultiple": false,
                            "type": "integer",
                            "required": false,
                            "minimum": 0,
                            "paramType": "query",
                            "description": "The number of items to return",
                            "name": "limit"
                        },
                        {
                            "format": "utc-millisec || alltime",
                            "allowMultiple": false,
                            "type": "integer",
                            "required": false,
                            "paramType": "query",
                            "description": "start of the range in which to return data",
                            "name": "from"
                        },
                        {
                            "format": "utc-millisec || now",
                            "allowMultiple": false,
                            "type": "integer",
                            "required": false,
                            "paramType": "query",
                            "description": "end of the range in which to return data",
                            "name": "to"
                        }
                    ],
                    "errorResponses": [
                        {
                            "code": 400,
                            "reason": "Invalid ID supplied"
                        },
                        {
                            "code": 404,
                            "reason": "account not found"
                        }
                    ]
                }
            ]
        }
    ],
    "models": {
